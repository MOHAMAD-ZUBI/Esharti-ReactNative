{
  "expo": {
    "name": "esharti",
    "slug": "esharti",
    "version": "1.7.0",
    "orientation": "portrait",
    "icon": "./src/assets/images/icon.png",
    "scheme": "esharti",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./src/assets/images/splash.png",
      "resizeMode": "cover",
      "backgroundColor": "#ffffff"
    },
    "notification": {
      "icon": "./src/assets/images/icon.png"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": false,
      "bundleIdentifier": "com.oxygen.esharty",
      "buildNumber": "5",
      "googleServicesFile": "./GoogleService-Info.plist",
      "associatedDomains": ["applinks:staging.esharti.net"],
      "infoPlist": {
        "ExpoLocalization_supportsRTL": true
      },
      "runtimeVersion": {
        "policy": "appVersion"
      }
    },
    "android": {
      "googleServicesFile": "./google-services.json",
      "softwareKeyboardLayoutMode": "pan",
      "adaptiveIcon": {
        "foregroundImage": "./src/assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.oxygen.esharty",
      "versionCode": 5,
      "runtimeVersion": "1.7.0",
      "intentFilters": [
        {
          "action": "VIEW",
          "autoVerify": true,
          "data": [
            {
              "scheme": "https",
              "host": "*.esharti.net",
              "pathPrefix": "/"
            }
          ],
          "category": ["BROWSABLE", "DEFAULT"]
        }
      ],
      "permissions": [
        "android.permission.READ_EXTERNAL_STORAGE",
        "android.permission.WRITE_EXTERNAL_STORAGE",
        "android.permission.DOWNLOAD_WITHOUT_NOTIFICATION",
        "android.permission.ACCESS_NETWORK_STATE"
      ]
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./src/assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-localization",
      "@react-native-firebase/app",
      "@config-plugins/react-native-blob-util",
      "@config-plugins/react-native-pdf",
      "./forceRTL",
      [
        "expo-build-properties",
        {
          "ios": {
            "useFrameworks": "static"
          }
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "supportsRTL": true,
      "eas": {
        "projectId": "fd7fc752-fb26-4a1d-8498-9a150f9fced0"
      }
    },
    "owner": "esharti",
    "updates": {
      "url": "https://u.expo.dev/fd7fc752-fb26-4a1d-8498-9a150f9fced0"
    }
  }
}
